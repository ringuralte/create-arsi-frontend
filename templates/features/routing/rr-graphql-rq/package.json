{
  "name": "rr-fs-graphql-rq",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.27.0",
  "description": "Template when using react router with react-router-fs-routes",
  "author": "ringuralte",
  "license": "ISC",
  "keywords": [],
  "scripts": {
    "dev": "react-router dev",
    "build:production": "react-router build",
    "build:staging": "NODE_ENV=test react-router build",
    "start": "react-router-serve ./build/server/index.js",
    "start:production": "PORT=3000 react-router-serve ./build/server/index.js",
    "start:staging": "NODE_ENV=test PORT=3001 react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "eslint . --fix",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@react-router/node": "^7.11.0",
    "@react-router/serve": "^7.11.0",
    "@tanstack/react-query": "^5.90.16",
    "graphql": "^16.12.0",
    "graphql-request": "^7.4.0",
    "isbot": "^5.1.32",
    "lucide-react": "^0.562.0",
    "nuqs": "^2.8.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.11.0",
    "vite-plugin-babel": "^1.3.2"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.15.2",
    "@antfu/eslint-config": "^6.7.3",
    "@eslint-react/eslint-plugin": "^2.5.1",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/client-preset": "^5.2.2",
    "@graphql-codegen/schema-ast": "^5.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@react-router/dev": "^7.11.0",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.2.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "react-router-devtools": "^6.0.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^6.0.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
