{
  "name": "rr-fs-graphql-rq",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.11.1",
  "description": "Template when using react router with react-router-fs-routes",
  "author": "ringuralte",
  "license": "ISC",
  "keywords": [],
  "scripts": {
    "dev": "react-router dev",
    "build:production": "react-router build",
    "build:staging": "NODE_ENV=test react-router build",
    "start": "react-router-serve ./build/server/index.js",
    "start:production": "PORT=3000 react-router-serve ./build/server/index.js",
    "start:staging": "NODE_ENV=test PORT=3001 react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "eslint . --fix",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@react-router/fs-routes": "^7.9.4",
    "@react-router/node": "^7.9.4",
    "@react-router/serve": "^7.9.4",
    "@tanstack/react-query": "^5.90.5",
    "graphql": "^16.11.0",
    "graphql-request": "^7.3.1",
    "isbot": "^5.1.31",
    "lucide-react": "^0.546.0",
    "nuqs": "^2.7.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.4",
    "vite-plugin-babel": "^1.3.2"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.15.0",
    "@antfu/eslint-config": "^6.1.0",
    "@eslint-react/eslint-plugin": "^2.2.3",
    "@graphql-codegen/cli": "^6.0.1",
    "@graphql-codegen/client-preset": "^5.1.0",
    "@graphql-codegen/schema-ast": "^5.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@react-router/dev": "^7.9.4",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.38.0",
    "eslint-plugin-format": "^1.0.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "react-router-devtools": "^5.1.3",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
