{
  "name": "react-router-fs",
  "version": "0.0.1",
  "description": "Template when using react router with react-router-fs-routes",
  "main": "index.js",
  "scripts": {
    "dev": "react-router dev",
    "build:production": "react-router build",
    "build:staging": "NODE_ENV=test react-router build",
    "start": "react-router-serve ./build/server/index.js",
    "start:production": "PORT=3000 react-router-serve ./build/server/index.js",
    "start:staging": "NODE_ENV=test PORT=3001 react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "eslint . --fix",
    "prepare": "husky",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "keywords": [],
  "author": "ringuralte",
  "license": "ISC",
  "packageManager": "pnpm@10.11.1",
  "dependencies": {
    "@react-router/fs-routes": "^7.9.4",
    "@react-router/node": "^7.9.4",
    "@react-router/serve": "^7.9.4",
    "date-fns": "^4.1.0",
    "graphql": "^16.11.0",
    "graphql-request": "^7.3.1",
    "isbot": "^5.1.31",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint-react/eslint-plugin": "^2.2.3",
    "@react-router/dev": "^7.9.4",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "eslint": "^9.38.0",
    "eslint-plugin-format": "^1.0.2",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "react-router-devtools": "^5.1.3",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-tsconfig-paths": "^5.1.4"
  }
