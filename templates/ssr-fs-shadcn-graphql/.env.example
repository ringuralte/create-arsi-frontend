# Environment Configuration
# Copy this file to .env.development and fill in your values

# Backend API base URL
VITE_BASE_URL=http://localhost:8002

# GraphQL endpoint URL
VITE_BASE_GRAPHQL_URL=http://localhost:8002/arsi

# Session cookie name (used for authentication)
VITE_SESSION_COOKIE_NAME=arsi-project

# Session secret (generate a secure random string)
VITE_SESSION_SECRET=your-secret-key-here

# Razorpay key (optional - for payment integration)
VITE_RZPAY_KEY=
